{
    "version": {
        "type": "str",
        "allowed_values": [
            "3d",
            "2d"
        ],
        "default": "3d",
        "fluent_required": true,
        "fluent_format": " {}"
    },
    "precision": {
        "type": "str",
        "allowed_values": [
            "single",
            "double"
        ],
        "default": "double",
        "fluent_required": true,
        "fluent_map": {
            "single": "",
            "double": "dp"
        },
        "fluent_format": "{}"
    },
    "processor_count": {
        "type": "int",
        "fluent_format": " -t{}"
    },
    "journal_filename": {
        "type": "str",
        "fluent_format": " -i {}"
    },
    "case_filepath": {
        "type": "str",
        "fluent_format": " -case {}"
    },
    "fluent_icing": {
        "type": "bool",
        "fluent_map": {
            "true": " -flicing",
            "false": ""
        },
        "fluent_format": "{}"
    }
}